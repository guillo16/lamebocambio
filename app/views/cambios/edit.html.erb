<br><br><br><br>
<div class="form-container">
  <h1>Editar</h1>

  <%= simple_form_for @cambio do |f| %>
  <%= f.error_notification %>

  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1554760381/flag-usa.jpg" alt="">
  <%= f.input :dolarc, label: 'Dolar Compra', required: true, value: @cambio.dolarc %>
  <%= f.input :dolarv, label: 'Dolar Venta', required: true, value: @cambio.dolarv %>
  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1554760381/flag-euro.jpg" alt="">
  <%= f.input :euroc, label: 'Euro Compra', required: true, value: @cambio.euroc %>
  <%= f.input :eurov, label: 'Euro Venta', required: true, value: @cambio.eurov %>
  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1554760381/gb.png" alt="">
  <%= f.input :gbpcompra, label: 'Libras Compra', required: true, value: @cambio.gbpcompra %>
  <%= f.input :gbpventa,label: 'Libras Venta', required: true, value: @cambio.gbpventa %>
  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1554760381/flag-br.jpg" alt="">
  <%= f.input :realc, label: 'Real Compra', required: true, value: @cambio.realc %>
  <%= f.input :realv,label: 'Real Venta', required: true, value: @cambio.realv %>
  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1554760381/flag-ur.jpg" alt="">
  <%= f.input :uruguayo, label: 'Uruguayo Compra', required: true, value: @cambio.uruguayo %>
  <%= f.input :uruguayov,label: 'Uruguayo Venta', required: true, value: @cambio.uruguayov %>
  <img src="https://res.cloudinary.com/dw7ox75dg/image/upload/v1559658494/flag-ch.jpg" alt="">
  <%= f.input :chilenoc, label: 'Chileno Compra', required: true, value: @cambio.chilenoc %>
  <%= f.input :chilenov,label: 'Chileno Venta', required: true, value: @cambio.chilenov %>

  <%= f.button :submit, "Actualizar Cambio", class: 'btn btn-primary' %>

  <% end %>

</div>

